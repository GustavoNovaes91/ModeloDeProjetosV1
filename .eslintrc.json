{
    "env": {
        "es2021": true,
        "node": true
    },
    "parser": "@typescript-eslint/parser",
    "extends":[
     "standard",
     "prettier"
     ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
     "@typescript-eslint",
     "prettier"
    ],
    "rules": {
        "prettier/prettier": "error"
    }
}
